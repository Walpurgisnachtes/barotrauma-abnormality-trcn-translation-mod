<?xml version="1.0" encoding="utf-8"?>
<!-- Thanks to Artifacts and Ruins Extended for this code -->
<Randomevents>
  <!-- SCRIPTED EVENTS -->
  <EventPrefabs>
    <ScriptedEvent identifier="warprecall">
      <TagAction criteria="player" tag="player" />
      <TagAction criteria="itemidentifier:warpteleportationanchor" tag="activeteleportationanchor" />
      <TriggerAction target1tag="recallteleporter" target2tag="player" applytotarget2="teleportplayer" radius="100" waitforinteraction="false" allowmultipletargets="false" />
      <TeleportAction position="MainSub" SpawnType="Human" SpawnPointTag="" targettag="teleportplayer" />
      <StatusEffectAction targettag="activeteleportationanchor">
        <StatusEffect target="This" disabledeltatime="true">
          <Use />
        </StatusEffect>
      </StatusEffectAction>
      <ClearTagAction tag="player" />
      <ClearTagAction tag="activeteleportationanchor" />
      <ClearTagAction tag="teleportplayer" />
    </ScriptedEvent>
    <!-- Wayward Passenger -->
    <ScriptedEvent identifier="waywardteleport">
      <TagAction criteria="speciesname:waywardpassenger" tag="waywardpassenger" />
      <TeleportAction position="MainSub" SpawnType="Human" SpawnPointTag="" targettag="waywardpassenger" />
      <WaitAction time="3" />
      <TeleportAction position="MainSub" SpawnType="Human" SpawnPointTag="" targettag="waywardpassenger" />
      <WaitAction time="3" />
      <TeleportAction position="MainSub" SpawnType="Human" SpawnPointTag="" targettag="waywardpassenger" />
      <ClearTagAction tag="waywardpassenger" />
    </ScriptedEvent>
    <!-- Blue Star -->
    <ScriptedEvent identifier="bluestarteleport">
      <TagAction criteria="speciesname:bluestar" tag="bluestar" />
      <TeleportAction position="MainSub" SpawnType="Human" SpawnPointTag="" targettag="bluestar" />
      <ClearTagAction tag="bluestar" />
    </ScriptedEvent>
    <!-- Traceless to Sight and Sound Alike -->
    <ScriptedEvent identifier="tracelesstosightandsoundalike">
      <TagAction criteria="player" tag="player" />
      <TriggerAction target1tag="recallteleporter_mao" target2tag="player" applytotarget2="teleportplayer" radius="100" waitforinteraction="false" allowmultipletargets="false" />
      <TeleportAction position="MainSub" SpawnType="Human" SpawnPointTag="" targettag="teleportplayer" />
      <RemoveItemAction itemidentifiers="maoteleport" />
      <ClearTagAction tag="player" />
      <ClearTagAction tag="teleportplayer" />
    </ScriptedEvent>
    <!-- NPC spawns -->
    <!-- Roland -->
    <ScriptedEvent identifier="washedupgrade9fixer">
